{
    "manifest_version": 2,
    "name": "My Extension",
    "description": "A sample Chrome Extension",
    "version": "1.0",
    "icons": {
      "128": "static/imgs/128.png"
    },
    "app": {
        "urls": [
        "http://certification.googleapps.com/app-info/"
        ],
        "launch": {
            "web_url": "http://certification.googleapps.com/"
        }
    },
    "browser_action": {
        "default_popup": "index.html"
      },
    "permissions": [
        "unlimitedStorage",
        "notifications",
        "activeTab"
    ]
}